<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/ll_skel"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <TableLayout
        android:id="@+id/skel_top"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff"
        android:shrinkColumns="*"
        android:stretchColumns="*" >

        <!-- area de registro do usuario -->

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:background="#dcdcdc"
            android:gravity="center_horizontal" >

            <TextView
                android:id="@+id/username_sessionData"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1.85"
                android:padding="30dip"
                android:text="Username_session"
                android:textColor="#000"
                android:textSize="14dp" />

            <ImageView
                android:id="@+id/img_logout"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_weight="0.15"
                android:onClick="logout"
                android:src="@drawable/ic_action_export" />
        </TableRow>
    </TableLayout>

    <ScrollView
        android:id="@+id/scrollView_skel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="0dp"
        android:layout_weight="1.01" >

        <!-- <LinearLayout -->
        <!-- android:id="@+id/ll_questionario" -->
        <!-- android:layout_width="match_parent" -->
        <!-- android:layout_height="match_parent" -->
        <!-- android:orientation="vertical" > -->
        <!-- </LinearLayout> -->

        <TableLayout
            android:id="@+id/skel_content"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#ffffff"
            android:shrinkColumns="*"
            android:stretchColumns="*" >

            <!-- botão responder questionario e fotinho -->

            <TableRow
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:gravity="center_horizontal" >

                <TextView
                    android:id="@+id/text1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="#00ff00"
                    android:text="multa"
                    android:textColor="#ff00ff"
                    android:textColorHighlight="#000000"
                    android:textIsSelectable="true"
                    android:textStyle="bold" />
            </TableRow>
        </TableLayout>
    </ScrollView>

    <TableLayout
        android:id="@+id/skel_bottom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff"
        android:shrinkColumns="*"
        android:stretchColumns="*" >

        <!-- botão responder questionario e fotinho -->

        <TableRow
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:gravity="center_horizontal" >

            <Button
                android:id="@+id/btn_voltar"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_weight="1"
                android:onClick="btn_voltar"
                android:text="Voltar" />

            <Button
                android:id="@+id/btn_prosseguir"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="10dp"
                android:layout_weight="1"
                android:onClick="btn_prosseguir"
                android:text="Imprimir" />
        </TableRow>
    </TableLayout>

</LinearLayout>